.nexStepWrap {
  height: 7vh;
  display: flex;
  text-align: center;
  padding: 0 20rpx;
  line-height: 7vh;
  position: relative;
}

.dataNum {
  position: relative;
  left: 50%;
  transform: translate(-50%);
  color: #ea9518;
  font-size: 28rpx;
}

.nextStep {
  margin-left: auto;
  width: 100rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 10rpx;
  background-color: #ea9518;
  color: #ffffff;
  font-size: 28rpx;
  position: relative;
  top: 50%;
  margin-top: -25rpx;
}

.imgListWrap {
  width: 100%;
  height: 93vh;
  background-color: rgba(0,0,0,.3);
  position: relative;
}

.imgData {
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%);
}

.selImg {
  position: relative;
}

.selImgWidth {
  width: 100%;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}

.selImgHeight {
  height: 100%;
  top: 0;
  left: 50%;
  transform: translate(-50%, 0);
}

.selVideo {
  width: 100%;
  position: relative;
  top: 40%;
  left: 0;
  transform: translate(0, -50%);
}